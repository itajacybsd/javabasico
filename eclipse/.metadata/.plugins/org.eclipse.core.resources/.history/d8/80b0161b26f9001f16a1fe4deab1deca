package com.loiane.cursojava.aula20;

public class Matrizes {

	public static void main(String[] args) {
	
		double[][] notaAlunos = new double[30][4];
		
		notaAlunos[0][0] = 10;
		notaAlunos[0][1] = 7;
		notaAlunos[0][2] = 9;
		notaAlunos[0][3] = 9.5;
		
		
		notaAlunos[1][0] = 9;
		notaAlunos[1][1] = 1;
		notaAlunos[2][2] = 4;
		notaAlunos[3][3] = 7.5;
		
		
		notaAlunos[2][0] = 9;
		notaAlunos[2][1] = 3;
		notaAlunos[2][2] = 2;
		notaAlunos[2][3] = 4.5;
		
		

	}

}
